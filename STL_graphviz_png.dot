f5:se-> f6:sw
f6:se-> f7:sw
f7:se-> f0:sw

fh [ fillcolor = "#D0FFFF", color = "#FFA0A0", label = "head" ]
fh:s -> f1:n[ weight = 300, color = "#D0FFFF", style = bold ]

ff [ fillcolor = "#D0FFFF", color = "#A0FFA0", label = "free" ]
